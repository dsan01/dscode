---
import PageTitle from '@generics/PageTitle.astro'
import { ProjectList } from '@sections/projects/ProjectList'
import FeaturedProject from '@sections/projects/FeaturedProject.astro'
import Layout from 'src/layouts/Layout.astro'
import ContactBanner from '@generics/ContactBanner.astro'
---

<Layout
  title={'meta.title.projetcs'}
  description={'meta.description.projects'}
>
  <PageTitle
    title='projects.page.title'
    description='projects.page.desc'
  />
  <FeaturedProject />
  <ProjectList
    url={Astro.url}
    client:only='react'
  />
  <ContactBanner
    title='projects.contact.title'
    description='projects.contact.desc'
    showDesc
  />
</Layout>
