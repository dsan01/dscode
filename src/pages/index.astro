---
import ServicesSlider from '@sections/index/ServicesSlider.astro'
import Layout from '../layouts/Layout.astro'
import Hero from '@sections/index/Hero.astro'
import ProjectHelp from '@sections/index/ProjectHelp.astro'
import ContactBanner from '@generics/ContactBanner.astro'
import TrajectorySection from '@generics/Trajectory'
import ProjectsList from '@sections/index/ProjectsList.astro'
import BlogList from '@sections/index/BlogList.astro'
---

<Layout
  title={'meta.title.home'}
  description={'meta.description.default'}
>
  <Hero />
  <ServicesSlider />
  <ProjectHelp />
  <ProjectsList />
  <BlogList />
  <TrajectorySection
    client:only='react'
    url={Astro.url}
  />
  <ContactBanner
    title={'index.contactBanner.title'}
    showDesc={false}
  />
</Layout>
